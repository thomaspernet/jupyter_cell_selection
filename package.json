{
  "name": "jupyter_cell_selection",
  "version": "0.1.0",
  "description": "A JupyterLab extension for marking cells for export with metadata",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/yourusername/jupyter_cell_selection",
  "bugs": {
    "url": "https://github.com/yourusername/jupyter_cell_selection/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Your Name",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jupyter_cell_selection.git"
  },
  "scripts": {
    "build": "tsc --skipLibCheck && jupyter labextension build .",
    "clean": "rimraf lib tsconfig.tsbuildinfo jupyter_cell_selection/labextension",
    "watch": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/cells": "^4.0.0",
    "@jupyterlab/docregistry": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@lumino/disposable": "^2.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyter_cell_selection/labextension"
  }
}
